<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Maui"
    xmlns:views="clr-namespace:Maui.Views"
    Title="Maui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent Title="Register" Route="RegisterPage" ContentTemplate="{DataTemplate views:RegisterPage}" />
    <ShellContent Title="Login" Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Title="Me" Route="MePage" ContentTemplate="{DataTemplate views:MePage}" />


</Shell>
